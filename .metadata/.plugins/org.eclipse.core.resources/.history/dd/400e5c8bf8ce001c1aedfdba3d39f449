package com.gl.javafsd.BSTtoSkewed;

public class DriverClass {
public static void main(String[] args) {
		BST tree = bst();
		BSTtoSkewed skewedTree = new BSTtoSkewed(tree);
        
}

private static BST bst() {
		
		BST tree = new BST();
		
		tree.insert(10);
		tree.insert(20);
		tree.insert(30);
		tree.insert(40);
		tree.insert(60);
		tree.insert(70);
		tree.insert(50);
		
	
		return tree;
}
}


